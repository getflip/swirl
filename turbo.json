{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**",
        "build/**",
        "loader/**",
        "storybook-static/**",
        "stencil-generated/**",
        ".next/**"
      ],
      "dependsOn": ["^build", "swirl-docs#build:spec-navigation"]
    },
    "test": {
      "dependsOn": []
    },
    "lint": {
      "outputs": [".eslintcache"]
    },
    "dev": {
      "cache": false,
      "outputs": [".next/**"],
      "dependsOn": [
        "@getflip/swirl-tokens#build",
        "@getflip/swirl-icons#build",
        "swirl-docs#build:spec-navigation"
      ]
    },
    "clean": {
      "cache": false
    },
    "gulp": {
      "outputs": ["dist/**"]
    },
    "build:spec-navigation": {
      "outputs": ["src/lib/navigation/src/data/apiDocs.data.ts"]
    },
    "build:swirl-docs": {
      "outputs": [".next/**"],
      "dependsOn": [
        "swirl-docs#build:spec-navigation",
        "^stencil:build",
        "@getflip/swirl-components-react#build"
      ]
    },
    "stencil:build": {
      "outputs": ["dist/**"],
      "dependsOn": ["@getflip/swirl-tokens#build"]
    }
  }
}
