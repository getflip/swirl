{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**",
        "build/**",
        "loader/**",
        "storybook-static/**",
        "stencil-generated/**",
        ".next/**"
      ],
      "dependsOn": ["^build"]
    },
    "test": {
      "dependsOn": []
    },
    "lint": {
      "outputs": [".eslintcache"]
    },
    "dev": {
      "cache": false,
      "outputs": [".next/**"],
      "dependsOn": ["@getflip/swirl-tokens#build", "@getflip/swirl-icons#build"]
    },
    "clean": {
      "cache": false
    },
    "gulp": {
      "outputs": ["dist/**"]
    },
    "prebuild": {
      "cache": false,
      "outputs": [
        "specs/**",
        "problem.yml",
        "shared.yml",
        "components.json",
        "src/lib/navigation/src/data/apiDocs.data.ts",
        "src/lib/navigation/src/data/apiSpecs.data.ts",
        "src/lib/navigation/src/data/apiEndpoints.data.ts"
      ]
    },
    "build:swirl-docs": {
      "cache": false,
      "outputs": [".next/**"],
      "dependsOn": [
        "prebuild",
        "^stencil:build",
        "@getflip/swirl-components-react#build"
      ]
    },
    "stencil:build": {
      "outputs": ["dist/**", "components.json"],
      "dependsOn": ["@getflip/swirl-tokens#build"]
    }
  }
}
