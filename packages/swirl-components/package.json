{
  "name": "@getflip/swirl-components",
  "version": "0.34.1",
  "description": "Swirl Design System Web Component Library",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/swirl-components/swirl-components.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/getflip/swirl"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist/",
    "loader/",
    "vscode-data.json",
    "components.json"
  ],
  "scripts": {
    "start": "yarn stencil:build && concurrently \"npm:*:start\"",
    "dev": "yarn start",
    "test": "yarn stencil:test",
    "build": "yarn stencil:build && yarn storybook:build",
    "generate": "plop",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "stencil:build": "stencil build",
    "stencil:start": "stencil build --watch",
    "stencil:test": "stencil test --spec",
    "stencil:test:watch": "stencil test --spec --watchAll",
    "stencil:test:coverage": "stencil test --spec --coverage --no-cache",
    "storybook:start": "DESIGN_TOKEN_GLOB=\"../../node_modules/@getflip/swirl-tokens/dist/css/styles.light.custom-properties.css\" start-storybook -p 6006 --no-manager-cache",
    "storybook:build": "DESIGN_TOKEN_GLOB=\"../../node_modules/@getflip/swirl-tokens/dist/css/styles.light.custom-properties.css\" build-storybook"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.0.1",
    "@getflip/swirl-icons": "^0.12.0",
    "@getflip/swirl-tokens": "^2.2.1",
    "@stencil/core": "^3.0.0",
    "a11y-dialog": "^7.5.2",
    "balance-text": "^3.3.1",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "^2.3.1",
    "color2k": "^2.0.0",
    "date-fns": "^2.29.3",
    "debounce-promise": "^3.1.2",
    "file-saver": "^2.0.5",
    "focus-trap": "^7.1.0",
    "maska": "^1.5.0",
    "papaparse": "^5.3.2",
    "pdfjs-dist": "^2.16.105",
    "sortablejs": "^1.15.0",
    "wc-datepicker": "^0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@pxtrn/storybook-addon-docs-stencil": "^6.4.1",
    "@stencil/angular-output-target": "^0.4.0",
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/postcss": "^2.1.0",
    "@stencil/react-output-target": "^0.3.1",
    "@storybook/addon-a11y": "^6.5.15",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-docs": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addons": "^6.5.15",
    "@storybook/builder-vite": "^0.3.0",
    "@storybook/html": "^6.5.15",
    "@storybook/manager-webpack4": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/theming": "^6.5.15",
    "@types/autoprefixer": "^10.2.0",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/debounce-promise": "^3.1.5",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^27.0.3",
    "@types/mdx": "^2.0.2",
    "@types/papaparse": "^5.3.5",
    "@types/sortablejs": "^1.15.0",
    "@typescript-eslint/parser": "^5.33.0",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "chromatic": "^6.7.4",
    "concurrently": "^7.3.0",
    "eslint-plugin-react": "^7.30.1",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "plop": "^3.1.1",
    "postcss-custom-media": "^8.0.2",
    "postcss-nested": "^5.0.6",
    "storybook-addon-themes": "^6.1.0",
    "storybook-design-token": "^2.8.2",
