{
  "source": ["./tokens/tokens.light.json"],
  "platforms": {
    "css": {
      "transformGroup": "css",
      "prefix": "s",
      "buildPath": "dist/css/",
      "files": [
        {
          "destination": "styles.light.custom-properties.css",
          "format": "css/variables-design-token-comments"
        }
      ]
    },
    "flutter": {
      "transformGroup": "flutter",
      "prefix": "",
      "buildPath": "dist/flutter/",
      "files": [
        {
          "className": "SwirlDesignTokens",
          "destination": "styles.light.dart",
          "format": "flutter/class.dart"
        }
      ]
    },
    "json": {
      "prefix": "s",
      "buildPath": "dist/",
      "files": [
        {
          "destination": "styles.light.json",
          "format": "json"
        }
      ]
    },
    "less": {
      "transformGroup": "less",
      "prefix": "s",
      "buildPath": "dist/less/",
      "files": [
        {
          "destination": "styles.light.less",
          "format": "less/variables"
        }
      ]
    },
    "scss": {
      "transformGroup": "scss",
      "prefix": "s",
      "buildPath": "dist/scss/",
      "files": [
        {
          "destination": "styles.light.scss",
          "format": "scss/variables"
        }
      ]
    },
    "tailwind": {
      "transformGroup": "css",
      "buildPath": "dist/tailwind/",
      "files": [
        {
          "destination": "light.json",
          "format": "css/tailwind"
        }
      ]
    }
  }
}
